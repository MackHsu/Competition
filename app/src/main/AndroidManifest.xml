<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.competition">

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.ConversationActivity"></activity>
        <activity
            android:name=".Activity.TestUpdateImgActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.MyFavoriteActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.MyRecruitmentActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.MyDiscussActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.NewDiscussActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.DiscussListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.TeamUpActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.DiscussActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.CompetitionDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.SignUpActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

</manifest>