<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.competition">

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MyFavoriteActivity"></activity>
        <activity android:name=".Activity.MyRecruitmentActivity" />
        <activity android:name=".Activity.MyDiscussActivity" />
        <activity android:name=".Activity.NewDiscussActivity" />
        <activity android:name=".Activity.DiscussListActivity" />
        <activity android:name=".Activity.TeamUpActivity" />
        <activity android:name=".Activity.DiscussActivity" />
        <activity android:name=".Activity.CompetitionDetailActivity" />
        <activity android:name=".Activity.SignUpActivity" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>